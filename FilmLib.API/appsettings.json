{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthorizationOptions": {
    "RolePermissions": [
      {"Role" : "User", "Permissions": ["Read"]},
      {"Role" : "Admin", "Permissions": ["Create", "Read", "Update", "Delete"]}
    ]
  }
}
